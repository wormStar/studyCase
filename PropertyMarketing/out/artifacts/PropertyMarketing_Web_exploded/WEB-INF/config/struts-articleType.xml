<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <package name="articleType" namespace="/articleType" extends="base-struts">
        <action name="*" class="articleTypeController" method="{1}">
            <result name="addPage">/WEB-INF/view/articleType/add.jsp</result>
            <result name="articleTypePager">/WEB-INF/view/articleType/adminPager.jsp</result>

            <result name="pager" type="json"></result>
            <result name="beUsable" type="json"></result>
            <result name="beDisable" type="json"></result>
            <result name="adminUpType" type="json"></result>
            <result name="addType" type="json"></result>
            <result name="allType" type="json"></result>

        </action>
    </package>

</struts>