<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <package name="activity" namespace="/activity" extends="base-struts">
        <action name="*" class="activityController" method="{1}">
            <result name="addPage">/WEB-INF/view/activity/add.jsp</result>
            <result name="activityPager">/WEB-INF/view/activity/adminPager.jsp</result>
            <result name="agencyAct">/WEB-INF/view/activity/agencyPager.jsp</result>
            <result name="listPager">/WEB-INF/view/activity/listPager.jsp</result>
            <result name="personInfor">/WEB-INF/view/activity/person.jsp</result>

            <result name="pager" type="json"></result>
            <result name="beUsable" type="json"></result>
            <result name="beDisable" type="json"></result>
            <result name="addActivity" type="json"></result>
            <result name="upActivity" type="json"></result>
            <result name="agencyPager" type="json"></result>

        </action>
    </package>

</struts>