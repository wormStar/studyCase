                            <li><a href="<%=path%>/user/userLoginPage">用户</a></li>
                            <li><a href="<%=path%>/employee/employeeLoginPager">员工</a></li>
                        </ul>
                    </div>
                </li>
            </c:otherwise>
        </c:choose>

    </ul>
</div>
<div class="edges">
        <span class=" logo">
            <a href="<%=path%>/home/homePager">
